CREATE TABLE IF NOT EXISTS images (
    job_id         varchar,
    seed           int,
    parameters     varchar,
    author_id      varchar,
    model_id       integer,
    insertion_time integer
);

CREATE TABLE IF NOT EXISTS user_settings (
    user_id         varchar,
    model_id        INTEGER,
    width           INTEGER,
    height          INTEGER,
    negative_prompt varchar,
    runpod_key      varchar
);