CREATE TABLE IF NOT EXISTS images (
    job_id         VARCHAR,
    seed           INTEGER,
    parameters     VARCHAR,
    author_id      VARCHAR,
    model_id       INTEGER,
    insertion_time INTEGER,
    alias          VARCHAR
);

CREATE TABLE IF NOT EXISTS user_settings (
    user_id         VARCHAR,
    model_id        INTEGER,
    width           INTEGER,
    height          INTEGER,
    negative_prompt VARCHAR,
    runpod_key      VARCHAR
);